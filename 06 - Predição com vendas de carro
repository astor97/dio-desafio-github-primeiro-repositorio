    {
      "cell_type": "code",
      "source": [
        "dados.rename(columns={'mileage_per_year': 'Quilometragem',\n",
        "                      'model_year'      : 'Ano do Carro',\n",
        "                      'price'           : 'Preço',         \n",
        "                      'sold'            : 'Vendido',\n",
        "                      'Unnamed: 0'      : 'ID'}, inplace = True)\n",
        "\n",
        "dados.loc[dados['Vendido'] == 'yes', 'Vendido'] = 1\n",
        "dados.loc[dados['Vendido'] == 'no', 'Vendido'] = 0"
      ],
